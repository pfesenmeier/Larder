@model List<string>
<div>
    <dl class="dl-horizontal">
        <dt>
            Seasons
        </dt>
        <dd>
            @if (Model.Count != 0)
            {
                @String.Join(", ", Model);
            }
            else
            {
                <text>Any</text>
            }
        </dd>
    </dl>
</div>